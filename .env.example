# Get you Convex deployment used by `npx convex dev`
CONVEX_DEPLOYMENT=
VITE_CONVEX_URL=

# Same as VITE_CONVEX_URL but ends in .site
VITE_CONVEX_SITE_URL=
VITE_SITE_URL=http://localhost:3000

# These must be set in your Convex environment for `convex/auth.ts`.
BETTER_AUTH_SECRET=
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=

# Frontend origin (used by Better Auth for redirects and origin checks).
SITE_URL=

# Get your OpenAI API Key here for chat models: https://platform.openai.com/account/api-keys
OPENAI_API_KEY=
